
public class Test04 {

	public static void main(String[] args) {
		// int(소숫점이 없는 정수) , double(소숫점이 있는 실수)
		// char(한글자==문자)  , String(문자열)
		// 변수를 선언하는 규칙 : 변수타입 네이밍(이름) ;
		
		int iNum;
		double dNum;
		char cha;
		String str;
		
		// 변수에 값 저장 (할당, 값) : 변수명 = 해당 타입에 맞는 값
		iNum=2;//정수
		dNum=3.5;//실수
		cha='정';//한 글자 담을 때 사용 '로 감싸주면됨
		str="수원";//
	}

}
