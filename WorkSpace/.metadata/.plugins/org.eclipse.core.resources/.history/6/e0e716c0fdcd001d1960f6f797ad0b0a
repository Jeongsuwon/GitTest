package pack07_escape;

public class Test01_Escape {

	public static void main(String[] args) {
		System.out.println("\\\t/\\");
		System.out.println(" )\t( ')");
		System.out.println(" (\t/)");
		System.out.println(" \\   (__)|");
		
		int a = 2;
		int b = 2;
		a += 1;
		System.out.println(a);
		
		
		
	}

}
