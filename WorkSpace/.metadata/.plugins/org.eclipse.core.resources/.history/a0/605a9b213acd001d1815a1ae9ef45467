package pack01_Variable;

public class Ex03_Variable {
//ctrl + shift + f == 줄정리
	public static void main(String[] args) {
		int value = 0;
		int result = value + 10; // value의 값을 알 수가 없어 오류 발생
	
		value = 30;
		
		result = value + 10; //40
		System.out.println(result);
		//숫자형 변수들은 사칙연산이 가능하다.
		//재할당. (변수를 계속 만드는게 아니라 값을 다시 줘서 바꿈)
		
		result = 10-2;
		System.out.println(result);
		
		
		
		
	}

}
