package pack04_char;

import java.util.Iterator;

public class Ex01_Char {

	public static void main(String[] args) {
		
		//문자 데이터 타입 : char(2byte)
		//하나의 글자만 저장 : 유니코드
		//변수에 값을 할당(대입)할 때는 작은 따옴표를 사용한다.
		
		char ch1 = 'a';
		System.out.println(ch1);
		
		char ch2 = '★';
		System.out.println(ch2);
		
		char ch3 = '가';
		System.out.println(ch3);
		
		
		
		
	}

}
