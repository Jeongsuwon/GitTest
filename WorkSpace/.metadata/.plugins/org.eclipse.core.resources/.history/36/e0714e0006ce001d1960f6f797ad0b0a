package pack08_casting;

public class Ex02_Casting {

	public static void main(String[] args) {
		
		//문자열 = "12" + 34 = 1234
		//String + int = String
		//why? 문자열 + 어떤값 = 문자열 규칙!
		//문자열 결합의 법칙 ↑
		
		String s1 = "12";
		int s = 34;
		System.out.println(s1+s);
		
		//어떤 값을 String으로 바꾸는 것은 엄청나게 간단하다.
		
		String sum = 1234+""+10;
		System.out.println(sum);
		
		
		
		
		
		
	}

}
